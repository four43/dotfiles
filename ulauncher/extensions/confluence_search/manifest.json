{
	"required_api_version": "^2.0.0",
	"name": "Confluence Wiki Opener",
	"description": "Confluence Wiki Opener",
	"developer_name": "Seth Miller",
	"icon": "images/icon.png",
	"options": {
		"query_debounce": 0.5
	},
	"preferences": [
		{
			"id": "project_kw",
			"type": "keyword",
			"name": "Project Keyword",
			"description": "The prefix keyword to use to start searching for projects",
			"default_value": "cf"
		},
		{
			"id": "subdomain",
			"type": "text",
			"name": "SubDomain",
			"description": "The subdomain for Confluence to use"
		},
		{
			"id": "email",
			"type": "text",
			"name": "Email",
			"description": "The Atlassian account email"
		},
		{
			"id": "api_token",
			"type": "text",
			"name": "API Token",
			"description": "An API token to use (https://id.atlassian.com/manage-profile/security/api-tokens)"
		}
	]
}
