    "explorer.autoReveal": "focusNoScroll",
    "security.workspace.trust.untrustedFiles": "open",
    "github.gitProtocol": "ssh",
    "remote.SSH.permitPtyAllocation": true,
    "docker.commands.build": "${containerCommand} buildx --pull --rm -f \"${dockerfile}\" -t ${tag} \"${context}\"",
    "docker.extension.dockerEngineAvailabilityPrompt": false,
    "workbench.editor.empty.hint": "hidden",
    "containers.commands.build": "${containerCommand} buildx --pull --rm -f \"${dockerfile}\" -t ${tag} \"${context}\"",
    "github.copilot.nextEditSuggestions.enabled": false,
    "docker.extension.enableComposeLanguageServer": false,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "python.testing.pytestEnabled": true,
    "chat.agent.maxRequests": 50,
    "chat.tools.terminal.autoApprove": {
        "cat": true,
        "cd": true,
        "echo": true,
        "ls": true,
        "pwd": true,
        "head": true,
        "tail": true,
        "findstr": true,
        "wc": true,
        "tr": true,
        "cut": true,
        "cmp": true,
        "which": true,
        "basename": true,
        "dirname": true,
        "realpath": true,
        "readlink": true,
        "stat": true,
        "file": true,
        "du": true,
        "df": true,
        "sleep": true,
        "git status": true,
        "git log": true,
        "git show": true,
        "git diff": true,
        "Get-ChildItem": true,
        "Get-Content": true,
        "Get-Date": true,
        "Get-Random": true,
        "Get-Location": true,
        "python -m pytest": true,
        "Write-Host": true,
        "Write-Output": true,
        "Split-Path": true,
        "Join-Path": true,
        "Start-Sleep": true,
        "Where-Object": true,
        "/^Select-[a-z0-9]/i": true,
        "/^Measure-[a-z0-9]/i": true,
        "/^Compare-[a-z0-9]/i": true,
        "/^Format-[a-z0-9]/i": true,
        "/^Sort-[a-z0-9]/i": true,
        "column": true,
        "/^column\\b.*-c\\s+[0-9]{4,}/": false,
        "date": true,
        "/^date\\b.*(-s|--set)\\b/": false,
        "find": true,
        "/^find\\b.*-(delete|exec|execdir|fprint|fprintf|fls|ok|okdir)\\b/": false,
        "grep": true,
        "/^grep\\b.*-(f|P)\\b/": false,
        "sort": true,
        "/^sort\\b.*-(o|S)\\b/": false,
        "tree": true,
        "/^tree\\b.*-o\\b/": false,
        "/\\(.+\\)/": {
            "approve": false,
            "matchCommandLine": true
        },
        "/\\{.+\\}/": {
            "approve": false,
            "matchCommandLine": true
        },
        "/`.+`/": {
            "approve": false,
            "matchCommandLine": true
        },
        "rm": false,
        "rmdir": false,
        "del": false,
        "Remove-Item": false,
        "ri": false,
        "rd": false,
        "erase": false,
        "dd": false,
        "kill": false,
        "ps": false,
        "top": false,
        "Stop-Process": false,
        "spps": false,
        "taskkill": false,
