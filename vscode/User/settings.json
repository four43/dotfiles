        "Activatable",
        "Aeris",
        "AERISAPI",
        "aerisweather",
        "BLOCKXSIZE",
        "BLOCKYSIZE",
        "Bookmarkable",
        "boto",
        "botocore",
        "dateutil",
        "dbus",
        "dedupe",
        "devcontainer",
        "dockerhub",
        "dtype",
        "gdalwarp",
        "geospatial",
