    "debug.toolBarLocation": "docked",
    "dev.containers.defaultExtensions": [
        // "charliermarsh.ruff",
        "anthropic.claude-code",
        "coolbear.systemd-unit-file",
        "donjayamanne.githistory",
        "mongodb.mongodb-vscode",
        "ms-toolsai.jupyter",
        "reduckted.vscode-gitweblinks",
        "ritwickdey.liveserver",
        "samuelcolvin.jinjahtml",
        "timonwong.shellcheck",
        "vivaxy.vscode-conventional-commits",
    ],
    "dev.containers.dockerComposePath": "docker compose",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontSize": 15,
    "editor.lineHeight": 25,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        120
    ],
    "editor.tokenColorCustomizations": {
        "[Nord Native]": {
            "textMateRules": [
                {
                    "scope": "entity.name.type.class",
                    "settings": {
                        "foreground": "#e9b696"
                    }
                },
                {
                    "scope": "entity.name.namespace",
                    "settings": {
                        "foreground": "#e9b696"
                    }
                },
                {
                    "scope": "variable.parameter.function.language.special.self.python",
                    "settings": {
                        "foreground": "#af8ede"
                    }
                },
                {
                    "scope": "variable.parameter.function.language.special.cls.python",
                    "settings": {
                        "foreground": "#af8ede"
                    }
                }
            ]
        }
    },
    "eslint.codeActionsOnSave.rules": null,
    "explorer.compactFolders": false,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.pytest_cache": true,
        "**/__pycache__": true,
        ".idea": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "git.commandsToLog": [
        "stash",
        "commit",
        "push",
        "branch",
        "pull"
    ],
    "git.confirmSync": false,
    "git.defaultBranchName": "master",
    "git.fetchOnPull": true,
    "git.inputValidation": true,
    "git.openRepositoryInParentFolders": "never",
    "git.rebaseWhenSync": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "js/ts.implicitProjectConfig.checkJs": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "markdown.extension.italic.indicator": "_",
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.extension.orderedList.marker": "one",
    "markdown.updateLinksOnFileMove.enabled": "always",
    "markdownlint.config": {
        "MD010": false,
        "MD012": false,
        "MD033": false,
        "MD041": false,
    },
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnusedVariable": "warning"
    },
    "python.analysis.inlayHints.callArgumentNames": "partial",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.packageIndexDepths": [
        {
            "depth": 10,
            "includeAllSymbols": true,
            "name": ""
        }
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.userFileIndexingLimit": -1,
    "python.experiments.enabled": false,
    "python.languageServer": "Pylance",
    "python.REPL.sendToNativeREPL": true,
    "redhat.telemetry.enabled": false,
    "remote.autoForwardPortsSource": "hybrid",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.fontFamily": "'DejaVuSansM Nerd Font'",
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.mouseWheelZoom": true,
    "terminal.integrated.rescaleOverlappingGlyphs": false,
    "terminal.integrated.tabs.location": "left",
    "window.newWindowDimensions": "maximized",
    "window.restoreFullscreen": true,
    "window.restoreWindows": "none",
    "window.title": "${activeRepositoryName}${separator}${remoteName}${separator}${activeEditorShort}${dirty}",
    "workbench.colorCustomizations": {
        "[Nord Native]": {
            "editorBracketHighlight.foreground1": "#5e81ac",
            "editorBracketHighlight.foreground2": "#88c0d0",
            "editorBracketHighlight.foreground3": "#a3be8c",
            "editorBracketHighlight.foreground4": "#ebcb8b",
            "editorBracketHighlight.foreground5": "#bf616a",
            "editorInlayHint.background": "#ffffff08",
            "editorInlayHint.foreground": "#ffffff2d",
            "gitDecoration.ignoredResourceForeground": "#909090"
        },
        "editorWhitespace.foreground": "#FFFFFF0F"
    },
    "workbench.editor.enablePreview": false,
    "workbench.list.openMode": "doubleClick",
    "workbench.tree.indent": 16,
    "workbench.trustedDomains.promptInTrustedWorkspace": true,
    "yaml.schemas": {
        "file:///workspace/machine-images/aws/docker-base/v1/files/opt/xweather-init/schemas/server-config.schema.json": "server-config*.yml",
        "file:///root/.vscode-server/extensions/atlassian.atlascode-3.6.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "file:/**/*.parquet": "jupyter-data-wrangler",
        "file:/**/*.csv": "jupyter-data-wrangler"
    },
    "makefile.configureOnOpen": true,
    "explorer.confirmPasteNative": false,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[svg]": {
        "editor.defaultFormatter": "jock.svg"
    },
